{
    "manifest_version": 2,
    "name": "ShowId",
    "version": "0.1",
    "description": "Fast Forward Ads",
	"content_scripts": [
		{
		  "matches": ["https://*/*"],
		  "js": ["js/fast.js"],
		  "css":["css/fast.css"]
		}
	  ],
    "permissions": [
         "contextMenus",
         "activeTab",
	 "declarativeContent"
       ],
    "icons": {
         "16": "icons/16.png",
         "32": "icons/32.png",
         "64": "icons/64.png"
         }
}